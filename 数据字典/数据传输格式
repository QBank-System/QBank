/Login
    request:
        work_number[varchar(20)]
        password[varchar(20)]
    return:
        state[1:登陆成功;0:登陆失败，学工号或密码错误;-1:未知原因，无法登录]
        personal[(state==1)用户信息,包含
            user_id[int]
            user_case[头像地址，url]
            user_work_number[学工号]
            user_password[密码]
            user_name[用户名]
            user_title[用户职称]
            user_college[所属学院]
            user_professional_field[专业领域]
            user_phone_number[电话号码]
            user_email[邮箱地址]
            user_office[办公地址]
            user_create_time[创建时间]
            user_grade[功能权限]
            ]

/Register
    request:
        user_name[]
        user_work_number[]
        user_password[]
        user_college[]
        user_phone_number[]
    return:
        state[1:注册成功;0:注册失败，学工号已存在;-1:未知原因，无法注册]
        personal[(state==1)用户信息,包含
            user_id[int]
            user_case[头像地址，url]
            user_work_number[学工号]
            user_password[密码]
            user_name[用户名]
            user_title[用户职称]
            user_college[所属学院]
            user_professional_field[专业领域]
            user_phone_number[电话号码]
            user_email[邮箱地址]
            user_office[办公地址]
            user_create_time[创建时间]
            user_grade[功能权限]
            ]

/SendVerificationCode
    request:
        phone_number[varchar(20)]
    return:
        state[1:发送成功;0:发送失败，手机号不存在;-1:未知原因，无法发送]
        code[(state==1)
            phone_number[手机号]
            verification_code[验证码]
            ]


